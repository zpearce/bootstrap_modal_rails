#ajax-modal.hide.modal.fade{:tabindex => "-1"}
    .modal-header
        %button.close{"aria-hidden" => "true", "data-dismiss" => "modal", :type => "button"} &times;
        %h3.modal-title Add New
    .modal-body
        .modal-body-content
        .progress.progress-info
    .modal-footer
        %button.btn{"aria-hidden" => "true", "data-dismiss" => "modal"} Close
        %button.btn.btn-primary.ajax-modal-submit Save changes
